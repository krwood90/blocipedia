<h1><%= @wikis.title %></h1>
<p><%= markdown(@wikis.body) %></p>

<h6>
    <small>submitted <%= time_ago_in_words(@wikis.created_at) %> ago by <%= @wikis.user.username %> <br></small>
</h6>
    <h4><%= link_to 'Edit Wiki', edit_wiki_path %></h4>
<% if current_user && current_user.admin? %>
    <h4><%= link_to 'Delete Wiki', @wikis, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete this wiki?'} %></h4>
<% end %>
<h4><%= link_to 'Back', wikis_path %></h4>
