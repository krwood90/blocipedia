<div class="row">
   <div class="col-md-8">
     <% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
            </br>
            <small>
             <%= wiki.body %>
           </small>
      </h4>
    </div>
  </div>
<% end %>
